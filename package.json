  "scripts": {
    "build": "babel src --out-dir lib",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:codyromano/react-collision-provider.git"
